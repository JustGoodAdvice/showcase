<div class="adviceset-scenarios"></div>


<script id="tmpl_advicesetScenarios" type="text/x-handlebars-template">
  {{#if scenarios}}
    <div class="list-group list-group-flush">
    {{#each scenarios}}
      <a href="{{#if shortUrl}}{{shortUrl}}{{else}}{{url}}{{/if}}" class="list-group-item list-group-item-action">{{name}}</a>
    {{/each}}
    </div>
  {{else}}
    No scenarios.
  {{/if}}
</script>
