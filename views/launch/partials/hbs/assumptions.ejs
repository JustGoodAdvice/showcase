<script id="tmpl_assumptionsList" type="text/x-handlebars-template">
{{#if _answersExist}}
	{{#each assumptions}}
		<div class="accordion-item">
			<h2 class="accordion-header" id="flush-heading-asmp{{@index}}">
				<button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
					data-bs-target="#flush-collapse-asmp{{@index}}" aria-expanded="false" aria-controls="flush-collapse-asmp{{@index}}">
					{{@key}}
				</button>
			</h2>
			<div id="flush-collapse-asmp{{@index}}" class="accordion-collapse collapse" aria-labelledby="flush-heading-asmp{{@index}}"
				data-bs-parent="#accordionFlushSidebar">
				<div class="accordion-body">
					<ul>
						{{#each this}}
						<li data-id="{{id}}" data-group-id="{{tagGroup.id}}" data-idx="{{idx}}" class="qa">
							{{#if statement}}
							<a href="#" class="statement">
								{{statement}}
							</a>
							{{else}}
							<span class="q">
								{{#if form.questionVariable}}
								<span class="text-monospace">{{form.questionVariable.name}}</span>&nbsp;&nbsp;&nbsp;
								{{else}}
								{{question}}
								{{/if}}
							</span>
							<span class="a"><a href="#" class="">
									{{#if answer}}
									{{answer}}
									{{else}}
									{{value}}
									{{/if}}
								</a></span>
							{{/if}}
						</li>
						{{/each}}
					</ul>
				</div>
			</div>
		</div>
	{{/each}}
{{/if}}
</script>



<script id="tmpl_assumptionsList" type="text/x-handlebars-template">
	{{#if _answersExist}}
		{{#each assumptions}}
			<div class="assumption-group"><a data-toggle="collapse" href="#collapse_{{@index}}" role="button" aria-expanded="false" aria-controls="collapse_{{@index}}"><i class="fal fa-chevron-{{#if this.0._isOpen}}down{{else}}right{{/if}}"></i>{{@key}}</a></div>
			<ol class="assumptions-list collapse {{#if this.0._isOpen}}show{{/if}}" id="collapse_{{@index}}">
				{{#each this}}
					<li data-id="{{id}}" data-group-id="{{tagGroup.id}}" data-idx="{{idx}}" class="qa">
						{{#if statement}}
							<a href="#" class="statement">
								{{statement}}
							</a>
						{{else}}
							<span class="q">
								{{#if form.questionVariable}}
									<span class="text-monospace">{{form.questionVariable.name}}</span>&nbsp;&nbsp;&nbsp;
								{{else}}
									{{question}}
								{{/if}}
							</span>
							<span class="a"><a href="#" class="">
								{{#if answer}}
									{{answer}}
								{{else}}
									{{value}}
								{{/if}}
							</a></span>
						{{/if}}
					</li>
				{{/each}}
			</ol>
		{{/each}}
	{{/if}}
</script>
