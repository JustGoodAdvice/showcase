<script type="text/javascript">
	window.jga=window.jga||{};
	window.jga._isMobile=<%= locals.isMobile || false %>;
	window.jga.api={
		adviceset:{
			id: "<%= locals.adviceSetId %>",
			_links: {
				self: "<%= locals.API_HOST %>/api/adviceset/<%= locals.adviceSetId.substring(2) %>",
				advicebuilder: "<%= locals.linkAdviceBuilder %>"
			}
		},
		_links:{self:"<%- locals.linkApi %>"}
	};
	window.jga.adviceSetId="<%= locals.adviceSetId %>";
</script>
<% if (locals.SENTRY_DSN) { %>
<script src="//browser.sentry-cdn.com/<%= locals.SENTRY_VERSION_APM %>/bundle.apm.min.js" crossorigin="anonymous"></script>
<script type="text/javascript">
	Sentry.init({ 
		dsn: '<%= locals.SENTRY_DSN %>',
		integrations: [
			new Sentry.Integrations.Tracing(),
		],
		tracesSampleRate: 0.5
	});
</script>
<% } %>
<script src="/jquery/dist/jquery.min.js" type="text/javascript"></script>
<script src="/bootstrap/dist/js/bootstrap.bundle.min.js" type="text/javascript"></script>
<script src="/dist/js/main.js" type="text/javascript"></script>
<% if (locals.INTERCOM_APP_ID) { %>
	<script>
		window.intercomSettings = {
			app_id: "<%= locals.INTERCOM_APP_ID %>",
			custom_launcher_selector: ".intercom_messenger_launcher"
		};
	</script>

	<script>
	(function(){var w=window;var ic=w.Intercom;if(typeof ic==="function"){ic('reattach_activator');ic('update',w.intercomSettings);}else{var d=document;var i=function(){i.c(arguments);};i.q=[];i.c=function(args){i.q.push(args);};w.Intercom=i;var l=function(){var s=d.createElement('script');s.type='text/javascript';s.async=true;s.src='https://widget.intercom.io/widget/<%= locals.INTERCOM_APP_ID %>';var x=d.getElementsByTagName('script')[0];x.parentNode.insertBefore(s,x);};if(w.attachEvent){w.attachEvent('onload',l);}else{w.addEventListener('load',l,false);}}})();
	</script>
<% } %>
