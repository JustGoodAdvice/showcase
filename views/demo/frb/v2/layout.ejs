<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  
  <% if (locals.api) { %>
    <title>
      <%= locals.api.data.adviceset.title %> - <%= locals.api.data.adviceset.owner.name %>
    </title>
    <meta name="description" content="<%= api.data.adviceset.description %>">
  <% } else { %>
    <title>Taffrail</title>
  <% } %>

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="application-name" content="Taffrail">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-title" content="Taffrail">
    <meta name="robots" content="index,follow">
    <meta name="google" content="nositelinkssearchbox">

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="//fonts.googleapis.com/css2?family=IBM+Plex+Sans:ital,wght@0,200;0,300;0,400;0,500;1,400&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/dist/css/taffrail.css" />

    <!--favicons-->
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">

    <meta name="turbolinks-cache-control" content="no-cache">
    <meta name="turbolinks-root" content="/frb">

    <script>window.jga = window.jga || {}; window.jga.config = { cdn_host: "<%= locals.CDN_HOST %>", web_host: "<%= locals.WEB_HOST %>", api_host: "<%= locals.API_HOST %>", advicebuilder_host: "<%= locals.ADVICEBUILDER_HOST %>", api_key: "<%= locals.API_KEY %>" };</script>

    <script src="/jquery/dist/jquery.min.js" type="text/javascript" defer></script>
    <script src="/bootstrap/dist/js/bootstrap.bundle.min.js" type="text/javascript" defer></script>
    <script src="/dist/js/frbwide.bundle.js" type="text/javascript" defer data-turbolinks-track="reload"></script>
    
    <% if (locals.SENTRY_DSN) { %>
      <script src="//browser.sentry-cdn.com/<%= locals.SENTRY_VERSION_APM %>/bundle.apm.min.js"
        crossorigin="anonymous"></script>
      <script type="text/javascript">
        Sentry.init({
          dsn: '<%= locals.SENTRY_DSN %>',
          integrations: [
            new Sentry.Integrations.Tracing(),
          ],
          tracesSampleRate: 0.5
        });
      </script>
    <% } %>
    
    <% if (locals.GTAG_ID) { %>
      <!-- Global site tag (gtag.js) - Google Analytics -->
      <script async src="//www.googletagmanager.com/gtag/js?id=<%= GTAG_ID %>"></script>
      <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', '<%= GTAG_ID %>');
      </script>
    <% } %>

    <script>window.jga.PERSONAS = <%- JSON.stringify(PERSONAS); %></script>
</head>

<body class="showcase showcase--redux bg-gradient-primary">

  <main class="container shadow-sm rounded-bottom">
    <header class="row">
      <!--<div class="col-md-5 col-lg-3"><img src="/img/Taffrail_Logo_Stacked_Navy@2x.png" height="100"></div>-->
      <div class="col-md-12">
        <div class="d-flex align-items-center justify-content-between">
          <div class="jumbotron px-0 w-100">
            <div class="containerx">
              <div class="advice-set-details">
                <h1><a href="/frb/v2/goal-planning/start">Goal Optimizer</a></h1>
                <div class="d-flex justify-content-between align-items-center">
                  <p class="lead">Proof-of-concept</p>
                </div>
              </div>
            </div>
          </div>
          <nav class="my-2 my-md-0 mr-md-3 d-flex align-items-center menubar">
            <!--
							<a class="p-2 text-dark" href="#">Features</a>
							<a class="p-2 text-dark" href="#">Enterprise</a>
							<a class="p-2 text-dark" href="#">About</a>
							-->
            <div class="dropdown" data-toggle="tooltip" title="View options">
              <a href="#" class="dropdown-toggle text-dark" id="dropdownMenuButton" data-toggle="dropdown"
                aria-haspopup="true" aria-expanded="false">
                <i class="fal fa-window-alt"></i>
              </a>
              <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton">
                <h6 class="dropdown-header">VIEWS</h6>
                <a class="dropdown-item active" href="#" data-target="full"><i class="fal fa-university"></i>&nbsp;Goal Optimizer</a>
                <a class="dropdown-item" href="#" data-target="full"><i class="fal fa-desktop"></i>&nbsp;Main</a>
                <a class="dropdown-item" href="#" data-target="harness"><i class="fal fa-desktop-alt"></i>&nbsp;Test
                  Harness</a>
                <a class="dropdown-item" href="#" data-target="mobile"><i class="fal fa-comments"></i>&nbsp;Mobile</a>
                <a class="dropdown-item" href="#" data-target="asst"><i class="fal fa-phone-laptop"></i>&nbsp;Virtual
                  Assistant</a>
                <a class="dropdown-item" href="#" data-target="salesforce"><i
                    class="fal fa-desktop-alt"></i>&nbsp;Salesforce</a>
                <div class="dropdown-divider"></div>
                <a class="dropdown-item" href="#" data-action="toggle-primary-advice-mode"><i
                    class="fal fa-comment-alt-edit"></i>&nbsp;Toggle Primary Advice Mode</a>
                <a class="dropdown-item" href="#" data-action="toggle-edit-advice-mode"><i
                    class="fal fa-edit"></i>&nbsp;Toggle Editor Mode</a>
              </div>
            </div>
            <div class="ml-3 dropdown" data-toggle="tooltip" title="Change voice">
              <a href="#" class="dropdown-toggle text-dark" id="dropdownMenuButton2" data-toggle="dropdown"
                aria-haspopup="true" aria-expanded="false">
                <i class="fal fa-microphone-alt"></i>
              </a>
              <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton2">
                <h6 class="dropdown-header">VOICES</h6>
                <div class="audience-switcher">
                  <a class="dropdown-item" href="#" data-audience-id="-1" data-action="set-audience"><i
                      class="fal fa-microphone-alt"></i>&nbsp;Default</a>
                  <a class="dropdown-item" href="#" data-audience-id="gRGmgTjX7" data-action="set-audience"><i
                      class="fal fa-microphone-alt"></i>&nbsp;Advisor</a>
                </div>
              </div>
            </div>
            <a href="#" class="ml-3 text-dark" data-action="modal-raw-data" data-toggle="tooltip" title="View data"><i
                class="fal fa-table"></i></a>
            <a href="#" class="ml-3 text-dark intercom_messenger_launcher" data-toggle="tooltip" title="Get support"><i
                class="fal fa-life-ring"></i></a>
          </nav>
        </div>
      </div>
    </header>

    <section class="row main-content">
      <div class="col-md-5 col-lg-3 assumptions-outer-container">
        <div class="assumptions-container">
          <div style="visibility:hidden;">
            <div class="tab-content" id="pills-tabContent">
              <div class="tab-pane fade show active" id="pills-assumptions" role="tabpanel"
                aria-labelledby="pills-assumptions-tab">
                <header class="assumptions-container-header">
                  <h5>&nbsp;</h5>
                  <div class="dropdown">
                    <a href="#" class="dropdown-toggle text-dark" id="dropdownMenuButton" data-toggle="dropdown"
                      aria-haspopup="true" aria-expanded="false">
                      <i class="fal fa-ellipsis-h"></i>
                    </a>
                    <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                      <a href="#" class="dropdown-item" data-expand="assumptions" data-collapsed="true"><i
                          class="fal fa-plus-square"></i> Expand all assumptions</a>
                      <a class="dropdown-item" id="pills-qa-tab" data-toggle="pill" href="#pills-qa" role="tab"
                        aria-controls="pills-qa" aria-selected="false"><i class="fal fa-comments"></i> Switch to Answer
                        view</a>
                    </div>
                  </div>
                </header>
                <div class="assumptions"></div>
              </div>
              <div class="tab-pane fade" id="pills-qa" role="tabpanel" aria-labelledby="pills-qa-tab">
                <header class="assumptions-container-header">
                  <h5>&nbsp;</h5>
                  <div class="dropdown">
                    <a href="#" class="dropdown-toggle text-dark" id="dropdownMenuButton" data-toggle="dropdown"
                      aria-haspopup="true" aria-expanded="false">
                      <i class="fal fa-ellipsis-h"></i>
                    </a>
                    <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                      <a class="dropdown-item" id="pills-assumptions-tab" data-toggle="pill" href="#pills-assumptions"
                        role="tab" aria-controls="pills-assumptions" aria-selected="false"><i
                          class="fal fa-comments"></i> Switch to Assumption view</a>
                    </div>
                  </div>
                </header>
                <div class="answers"></div>
              </div>
            </div>
          </div>
          <footer>
            <div class="logo"><a href="https://taffrail.com" target="_blank"><img src="/img/Taffrail_Logo_Stacked_Navy@2x.png" height="100"></a></div>
          </footer>
        </div>
      </div>
      <div class="col-md-7 col-lg-9 advice-outer-container">
        <!--<div class="center-ph-loader">
          <%- include("index/loading"); %>
        </div>-->
        <div class="row">
          <div class="col-lg-7 center-col">
            <!-- use this script in the <body> so turbolinks executes it each page load -->
            <script async="false">
                window.jga.api = {
                adviceset: {
                  id: "<%= locals.adviceSetId %>",
                    _links: {
                    self: "<%= locals.API_HOST %>/api/adviceset/<%= locals.adviceSetId.substring(2) %>",
                      advicebuilder: "<%= locals.linkAdviceBuilder %>"
                  }
                },
                _links: { self: "<%- locals.linkApi %>" }
              };
              window.jga.adviceSetId = "<%= locals.adviceSetId %>";
            </script>
            
            <%-body %>
            <!--
            <div class="advice"></div>
            <div class="list-all-recommendations"></div>
            -->
          </div>
          <div class="col-lg-5 right-col">
            <div class="advice-debug"></div>
          </div>
        </div>
      </div>

    </section>
  </main>
  <%- include("partials/_footer"); %>
  <%- include("partials/_modal_vars"); %>

  <!--templates-->
  <%- include("index/adviceSetDetails"); %>
  <%- include("index/inputRequest"); %>
  <%- include("index/advice"); %>
  <%- include("index/recommendations"); %>
  <%- include("index/recommendationsToc"); %>
  <%- include("partials/error"); %>

  <%- include("index/answers"); %>
  <%- include("index/assumptions"); %>
  <%- include("index/variables"); %>
  <%- include("partials/_toast"); %>

  <div id="__init" data-page-init="showcase"></div>

</body>

</html>
